#include <stdio.h>

 main() {
    int size, i, sum = 0;

    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int arr[size];

    printf("Enter the array elements:\n");

    for (i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
        sum =sum+arr[i];
    }

    printf("Sum of array elements: %d", sum);

  
}
